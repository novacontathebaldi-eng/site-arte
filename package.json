  "private": true,
  "scripts": {
    "clean-legacy": "node -e \"const fs = require('fs'); const path = require('path'); ['index.html', 'index.tsx', 'vite.config.ts'].forEach(f => { const p = path.join(process.cwd(), f); if(fs.existsSync(p)) { try { fs.rmSync(p, { force: true }); console.log('REMOVED LEGACY FILE:', p); } catch(e) { console.error('FAILED TO REMOVE:', p, e); } } });\"",
    "dev": "npm run clean-legacy && next dev",
    "prebuild": "npm run clean-legacy",
    "build": "npm run clean-legacy && next build",
    "start": "next start",
    "lint": "next lint"
  },
